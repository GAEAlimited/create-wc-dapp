{
	"name": "create-wc-dapp",
	"version": "0.0.1",
	"keywords": [
		"create-wc-dapp",
		"create wc dapp",
		"boilerplate",
		"WalletConnect"
	],
	"type": "module",
	"scripts": {
		"dev": "nodemon ./index.js",
		"lint": "eslint . --ext .js && yarn prettier --write .",
		"start": "npx create-wc-dapp",
		"dev:norestart": "node ./index.js && rm -rf my-wc-dapp/",
		"dev:nst": "cd core/next-starter-template && yarn dev"
	},
	"bin": {
		"create-wc-dapp": "./index.js"
	},
	"description": "Bootstrap a WalletConnect integrated dapp with with one command",
	"license": "MIT",
	"devDependencies": {
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"nodemon": "^2.0.21",
		"prettier": "^2.8.7"
	},
	"dependencies": {
		"chalk": "^5.2.0",
		"cli-progress": "^3.12.0",
		"dotenv": "^16.0.3",
		"fs-extra": "^11.1.0",
		"prompts": "^2.4.2",
		"yargs": "^17.7.1"
	}
}
